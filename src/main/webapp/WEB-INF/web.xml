<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="https://jakarta.ee/xml/ns/jakartaee"
xmlns:web="https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
		    https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>Linked Eurostat</display-name>

  <listener>
		<listener-class>com.ontologycentral.estatwrap.webapp.Listener</listener-class>
	</listener>
	<!--
	<filter>
		<filter-name>Compress</filter-name>
    	<filter-class>com.ontologycentral.estatwrap.webapp.GzipFilter</filter-class>
	</filter>

	<filter-mapping>
    	<filter-name>Compress</filter-name>
    	<url-pattern>/*</url-pattern>
	</filter-mapping>
-->
	<servlet>
		<servlet-name>id</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.IdentifierServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>page</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.PageServlet</servlet-class>
	</servlet>
		<servlet>
		<servlet-name>timeline</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.TimelineServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>cs</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.CsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ds</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.DsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>df</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.DfServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>dc</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.DcServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>cl</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.ClServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>da</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.DaServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>feed</servlet-name>
		<servlet-class>com.ontologycentral.estatwrap.webapp.FeedServlet</servlet-class>
	</servlet>
	

	<servlet-mapping>
		<servlet-name>id</servlet-name>
		<url-pattern>/id/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>page</servlet-name>
		<url-pattern>/page/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>timeline</servlet-name>
		<url-pattern>/vis/timeline</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet-mapping>
		<servlet-name>cs</servlet-name>
		<url-pattern>/cs/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>cl</servlet-name>
		<url-pattern>/cl/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ds</servlet-name>
		<url-pattern>/ds/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>df</servlet-name>
		<url-pattern>/df/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>dc</servlet-name>
		<url-pattern>/dc/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>da</servlet-name>
		<url-pattern>/da/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>feed</servlet-name>
		<url-pattern>/feed.rdf</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
